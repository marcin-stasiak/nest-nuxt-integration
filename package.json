{
  "name": "nest-nuxt-integration",
  "version": "1.0.3",
  "description": "Nest Nuxt integration",
  "author": "Marcin Stasiak",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "env NODE_ENV=production node app.js",
    "build": "tsc -p tsconfig.json && nuxt-ts build",
    "dev": "env NODE_ENV=development nodemon",
    "dev:client": "nuxt-ts -p 3001",
    "dev:server": "nest start --watch"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nuxt/typescript-runtime": "^0.4.0",
    "@nuxtjs/axios": "^5.3.6",
    "nuxt": "^2.0.0",
    "nuxt-property-decorator": "^2.7.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nuxt/typescript-build": "^0.6.0",
    "@types/node": "^13.11.0",
    "nodemon": "^2.0.0",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0"
  }
}
